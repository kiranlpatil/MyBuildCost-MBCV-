<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tpl.mobileseed" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MobileSeed</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~5.1.1" />
    <plugin name="phonegap-facebook-plugin" spec="C:\Users\lenovo\Downloads\phonegap-facebook-plugin-master">
        <variable name="APP_ID" value="1802774049959105" />
        <variable name="APP_NAME" value="tpl_mobile_seed" />
    </plugin>
    <plugin name="cordova-plugin-backbutton" spec="~0.3.0" />
    <plugin name="cordova-plugin-camera" spec="~2.2.0" />
    <plugin name="cordova-plugin-sqlite-2" spec="~1.0.4" />
    <plugin name="cordova-plugin-filepath" spec="~1.0.2" />
    <plugin name="cordova-plugin-network-information" spec="~1.2.1" />
</widget>
