    "External plaster" : "carpetArea * 2.5 * 45",
    "Waterproofing" : "carpetArea * 1.35 * 30",
    "Dado / wall tiling" : "carpetArea * 1.35 * 110",   //a
    "windowSills/Jambs" : "carpetArea * 0.07 * 85", //both
    "windowsAndGlassDoors" : "250 * (carpetArea * 0.2)",
    "electrification" : "carpetArea * 1.35 * 110",
    "Fabrication" : "carpetArea * 1.35 * 40",   //a
    "Pointing" : "carpetArea * 1.35 * 55",    // a
    "Kitchen otta" : "(numOfOneBHK + numOfTwoBHK + numOfThreeBHK + numOfFourBHK + numOfFiveBHK) * 15 * 1500",    //a
    "Door Frames" : "10000 * (3 * numOfOneBHK + 4 * numOfTwoBHK + 6 * numOfThreeBHK + 8 * numOfFourBHK + 10 * numOfFiveBHK) + 1500 * (numOfOneBHK + numOfTwoBHK + numOfThreeBHK + numOfFourBHK + numOfFiveBHK)", //a door frames
    "External plumbing / drainage line" : "5000 * (numOfOneBHK * 1 + numOfTwoBHK * 2 + numOfThreeBHK * 3 + numOfFourBHK * 4 + numOfFiveBHK * 5) + 6000 * (numOfOneBHK + numOfTwoBHK + numOfThreeBHK + numOfFourBHK + numOfFiveBHK)",   //plumbing
    "lift" : "(150000 * numOfFloors * numOfLifts)",
    "electricalLightFittings" : "((carpetArea / 400) * 500 + ((projectPeriphery / 40) * 5000 + ( numOfFloors * 3) * 700)",
    "garbageChute" : "(60000 * (numOfFloors - numOfParkingFloors))",
    "modularKitchenOrFurniture" : "(numOfOneBHK + numOfTwoBHK + numOfThreeBHK + numOfFourBHK + numOfFiveBHK) * 125000",
    "pestControl" : "carpetArea * 1.35 * 2",
    "solarWater" : "carpetArea * 1.35 * 20",
    "pipedGasSystem" : "12000 * (numOfOneBHK + numOfTwoBHK + numOfThreeBHK + numOfFourBHK + numOfFiveBHK) + 300000",
    "skyLounge" : "(slabArea / (numOfFloors + numfOfParkingFloors)) * 150",
    "fireFightingSystem" : "( 50000 * numOfFloors)",
    "securityAutomation" : "15000 * (numOfOneBHK + numOfTwoBHK + numOfThreeBHK + numOfFourBHK + numOfFiveBHK) + 3500 * numOfBuildings + 3500 * 2 + 15000",
    "showerInclosure" : "15000 * (1 * numOfOneBHK + 1 * numOfTwoBHK + 2 * numOfThreeBHK + 3 * numOfFourBHK + 4 * numOfFiveBHK)",
    "falseCeiling" : "100 * (40 * numOfOneBHK + 80 * numOfTwoBHK + 120 * numOfThreeBHK + 160 * numOfFourBHK + 200 * numOfFiveBHK)",
    "specialElevationalFeatures" : "carpetArea * 1.35 * 25",
